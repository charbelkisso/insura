<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" xmlns:sap="http://www.sap.com/Protocols/SAPData">
  <edmx:DataServices m:DataServiceVersion="2.0">
    <Schema Namespace="CompanyService" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      <EntityContainer Name="EntityContainer" m:IsDefaultEntityContainer="true">
        <EntitySet Name="Companies" EntityType="CompanyService.Companies"/>
        <EntitySet Name="Partners" EntityType="CompanyService.Partners"/>
        <AssociationSet Name="Companies_partner" Association="CompanyService.Companies_partner">
          <End Role="Companies" EntitySet="Companies"/>
          <End Role="Partners" EntitySet="Partners"/>
        </AssociationSet>
      </EntityContainer>
      <EntityType Name="Companies">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <NavigationProperty Name="partner" Relationship="CompanyService.Companies_partner" FromRole="Companies" ToRole="Partners"/>
        <Property Name="partner_ID" Type="Edm.Guid"/>
        <Property Name="contactPerson_ID" Type="Edm.Guid"/>
      </EntityType>
      <EntityType Name="Partners">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <Property Name="name1" Type="Edm.String"/>
        <Property Name="name2" Type="Edm.String"/>
        <Property Name="name3" Type="Edm.String"/>
        <Property Name="address_ID" Type="Edm.Guid"/>
        <Property Name="type_ID" Type="Edm.Int32"/>
      </EntityType>
      <Association Name="Companies_partner">
        <End Role="Companies" Type="CompanyService.Companies" Multiplicity="*"/>
        <End Role="Partners" Type="CompanyService.Partners" Multiplicity="0..1">
          <OnDelete Action="Cascade"/>
        </End>
        <ReferentialConstraint>
          <Principal Role="Partners">
            <PropertyRef Name="ID"/>
          </Principal>
          <Dependent Role="Companies">
            <PropertyRef Name="partner_ID"/>
          </Dependent>
        </ReferentialConstraint>
      </Association>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>