<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata" xmlns:sap="http://www.sap.com/Protocols/SAPData">
  <edmx:DataServices m:DataServiceVersion="2.0">
    <Schema Namespace="PersonService" xmlns="http://schemas.microsoft.com/ado/2008/09/edm">
      <EntityContainer Name="EntityContainer" m:IsDefaultEntityContainer="true">
        <EntitySet Name="Partners" EntityType="PersonService.Partners"/>
        <EntitySet Name="Persons" EntityType="PersonService.Persons"/>
        <AssociationSet Name="Persons_partner" Association="PersonService.Persons_partner">
          <End Role="Persons" EntitySet="Persons"/>
          <End Role="Partners" EntitySet="Partners"/>
        </AssociationSet>
      </EntityContainer>
      <EntityType Name="Partners">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <Property Name="name1" Type="Edm.String"/>
        <Property Name="name2" Type="Edm.String"/>
        <Property Name="name3" Type="Edm.String"/>
        <Property Name="address_ID" Type="Edm.Guid"/>
        <Property Name="type_ID" Type="Edm.Int32"/>
      </EntityType>
      <EntityType Name="Persons">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <NavigationProperty Name="partner" Relationship="PersonService.Persons_partner" FromRole="Persons" ToRole="Partners"/>
        <Property Name="personalNumber" Type="Edm.String" MaxLength="12"/>
        <Property Name="birthDate" Type="Edm.DateTime" sap:display-format="Date"/>
        <Property Name="contactInfo_phoneNumber" Type="Edm.String" MaxLength="40"/>
        <Property Name="contactInfo_mobilNumber" Type="Edm.String" MaxLength="40"/>
        <Property Name="contactInfo_email" Type="Edm.String" MaxLength="100"/>
        <Property Name="partner_ID" Type="Edm.Guid"/>
      </EntityType>
      <Association Name="Persons_partner">
        <End Role="Persons" Type="PersonService.Persons" Multiplicity="*"/>
        <End Role="Partners" Type="PersonService.Partners" Multiplicity="0..1">
          <OnDelete Action="Cascade"/>
        </End>
        <ReferentialConstraint>
          <Principal Role="Partners">
            <PropertyRef Name="ID"/>
          </Principal>
          <Dependent Role="Persons">
            <PropertyRef Name="partner_ID"/>
          </Dependent>
        </ReferentialConstraint>
      </Association>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>